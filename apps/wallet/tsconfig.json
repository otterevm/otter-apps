{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"strict": true,
		"jsx": "react-jsx",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"paths": {
			"#*": ["./src/*"],
			"#wagmi.config": ["./src/wagmi.config.ts"]
		},
		"types": ["vite/client", "unplugin-icons/types/react"]
	},
	"include": ["src/**/*"]
}
