# Index Supply API key (server-side)
INDEXER_API_KEY=""

# RPC auth key (server-side)
TEMPO_RPC_KEY=""

VITE_BASE_URL=""

# Set to "1" to require basic HTTP auth, forwarded to the RPC for validation
# FORWARD_RPC_AUTH=""

# Sentry (server + client)
# SENTRY_DSN=""
# SENTRY_AUTH_TOKEN=""
# SENTRY_ORG=""
# SENTRY_PROJECT=""
# SENTRY_TRACES_SAMPLE_RATE="0.1"
# VITE_SENTRY_DSN=""
# VITE_SENTRY_TRACES_SAMPLE_RATE="0.1"

# --------------------------------------------------------
# The following vars are set automatically per environment
# in wrangler.jsonc and are generally not to be set here.
# --------------------------------------------------------
# VITE_TEMPO_ENV="moderato"  # devnet | moderato | testnet | presto | custom

# --------------------------------------------------------
# Custom Chain Configuration (for docker-compose deployments)
# Use these when VITE_TEMPO_ENV=custom
# --------------------------------------------------------
# VITE_CHAIN_NAME="Pakxe"                    # Blockchain name (e.g., Pakxe, AISPoint)
# VITE_CHAIN_ID="7447"                       # Chain ID
# VITE_RPC_URL="https://rpc.pakxe.otterevm.com/"  # RPC endpoint
# VITE_LOGO_URL="/logo.svg"                  # Logo URL (optional, defaults to /logo.svg)

# demo screens
# VITE_ENABLE_DEMO=false

# show react query + react router devtools
# VITE_ENABLE_DEVTOOLS=true

# enable light/dark mode keyboard toggle
# VITE_ENABLE_COLOR_SCHEME_TOGGLE=false

# (dev/preview) comma-separated list of allowed hosts to use in vite config. See
# https://vite.dev/config/server-options
# https://vite.dev/config/preview-options#preview-allowedhosts
# ALLOWED_HOSTS=""
